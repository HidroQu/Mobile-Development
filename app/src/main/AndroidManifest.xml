<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HidroQu"
        tools:targetApi="31">
        <activity
            android:name=".ui.comunity.ComunityActivity"
            android:exported="false"
            android:label="@string/title_activity_comunity"
            android:theme="@style/Theme.HidroQu" />
        <activity
            android:name=".ui.profile.ProfileActivity"
            android:exported="false"
            android:label="@string/title_activity_profile"
            android:theme="@style/Theme.HidroQu" />
        <activity
            android:name=".ui.myplant.MyPlantActivity"
            android:exported="false"
            android:label="@string/title_activity_my_plant"
            android:theme="@style/Theme.HidroQu" />
        <activity
            android:name=".ui.article.ArticleActivity"
            android:exported="false"
            android:label="@string/title_activity_article"
            android:theme="@style/Theme.HidroQu" />
        <activity
            android:name=".ui.home.HomeActivity"
            android:exported="false"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.HidroQu" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.HidroQu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>